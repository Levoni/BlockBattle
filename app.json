{
  "expo": {
    "name": "BlockBattle",
    "description": "Multiplayer Blokus Game.",
    "slug": "BlockBattle",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.levon_swenson.BlockBattle",
      "adaptiveIcon": {
        "foregroundImage" : "./assets/icon.png",
        "backgroundColor": "#00ffff"
      }
    }
  }
}
